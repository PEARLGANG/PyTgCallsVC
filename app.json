{
    "name": "TG-VoiceChat-VideoStreamer",
    "description": "A Pyrogram Telegram bot to stream videos to voice chats..",
    "keywords": [
        "telegram",
        "stream",
        "video-calls",
        "pyrogram",
        "group-calls",
        "python",
        "plugin",
        "modular",
        "pytgcalls"
      ],
    "repository": "https://github.com/EverythingSuckz/tgvc-video-tests",
    "success_url": "/",
    "logo": "https://telegra.ph/file/8b9cfbe9aaf44f7ff4468.jpg",
    "website": "https://github.com/EverythingSuckz/tgvc-video-tests",
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "Get this value from https://my.telegram.org"
            "required": false
        },
        "API_HASH": {
            "description": "Get this value from https://my.telegram.org"
            "required": false
        },
        "SUDO": {
            "description": "User IDs of users who have access to use the bot commands. Separate each with space."
            "required": false
        },
        "SESSION": {
            "description": "Get this value from https://replit.com/@ayrahikari/pyrogram-session-maker"
            "required": false
        },
        "BOT_TOKEN": {
            "description": "Get this value from @BotFather",
            "required": false
        }
    },
    "buildpacks": [{
        "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
      }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
  }
